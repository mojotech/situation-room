{
  "arrow_spacing": {
    "level": "error"
  },
  "braces_spacing": {
    "spaces": 1,
    "empty_object_spaces": 0,
    "level": "error"
  },
  "colon_assignment_spacing": {
    "spacing": {
      "left": 0,
      "right": 1
    },
    "level": "error"
  },
  "eol_last": {
    "level": "error"
  },
  "indentation": {
    "value": 2,
    "level": "error"
  },
  "line_endings": {
    "level": "error"
  },
  "max_line_length": {
    "value": 120,
    "level": "error"
  },
  "no_debugger": {
    "level": "error"
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "no_stand_alone_at": {
    "level": "error"
  },
  "no_tabs": {
    "level": "error"
  },
  "no_trailing_semicolons": {
    "level": "error"
  },
  "no_trailing_whitespace": {
    "level": "error"
  },
  "no_unnecessary_fat_arrows": {
    "level": "error"
  },
  "prefer_double_quotes": {
      "module": "coffeelint-prefer-double-quotes",
      "level": "error"
  },
  "prefer_english_operator": {
    "level": "error"
  },
  "spacing_after_comma": {
    "level": "error"
  },
  "space_operators": {
    "level": "error"
  }
}
